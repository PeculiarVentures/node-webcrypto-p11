    "compilerOptions": {
        "target": "es2019",
        "noUnusedLocals": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "removeComments": true,
        "strict": true,
        "experimentalDecorators": true,
        "importHelpers": true
    },
    "exclude": [
        "build"
    ]
}