{
    "typescript.tsdk": "node_modules/typescript/lib",
    "cSpell.words": [
        "Cryptoki",
        "graphene"
    ],
    "files.trimTrailingWhitespace": true,
    "typescript.format.semicolons": "insert",
    "typescript.preferences.quoteStyle": "double",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/build": true,
    }
}